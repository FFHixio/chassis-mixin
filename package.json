{
  "name": "chassis-mixins",
  "version": "0.0.5",
  "description": "A DOM element extension library.",
  "main": "core.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "karma start karma.conf.js && standard --verbose | snazzy",
    "gutcheck": "BUILD_MODE=live karma start karma.conf.js && standard --verbose | snazzy",
    "fulltest": "BUILD_MODE=prod karma start karma.conf.js && standard --verbose | snazzy",
    "firefox": "karma start karma.conf.js --firefox && standard --verbose | snazzy",
    "safari": "karma start karma.conf.js --safari && standard --verbose | snazzy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngnjs/chassis-mixin.git"
  },
  "keywords": [
    "ngn",
    "chassis",
    "web",
    "component",
    "dom"
  ],
  "author": "Author.io",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ngnjs/chassis-mixin/issues"
  },
  "homepage": "https://github.com/ngnjs/chassis-mixin#readme",
  "devDependencies": {
    "babel-preset-es2015": "6.9.0",
    "browserify": "13.0.0",
    "buffer-equal-constant-time": "1.0.1",
    "caniuse-api": "1.5.0",
    "del": "2.2.1",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-concat": "2.6.0",
    "gulp-header": "1.8.7",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "1.5.4",
    "gulp-vinyl-zip": "^1.2.2",
    "karma": "6.3.16",
    "karma-browserify": "5.0.3",
    "karma-chrome-launcher": "0.2.3",
    "karma-firefox-launcher": "0.1.7",
    "karma-html2js-preprocessor": "0.1.0",
    "karma-ie-launcher": "0.2.0",
    "karma-opera-launcher": "0.3.0",
    "karma-phantomjs-launcher": "1.0.1",
    "karma-safari-launcher": "0.1.1",
    "karma-sauce-launcher": "1.0.0",
    "karma-spec-reporter": "0.0.26",
    "karma-tap": "1.0.4",
    "localenvironment": "^1.0.2",
    "musthave": "^1.0.2",
    "phantomjs-prebuilt": "2.1.7",
    "publish-release": "^1.3.1",
    "pump": "1.0.1",
    "shortbus": "^1.0.6",
    "snazzy": "3.0.0",
    "standard": "6.0.8",
    "tape": "4.6.0",
    "through2": "2.0.1",
    "uglify-js": "2.6.4",
    "watchify": "3.7.0"
  },
  "standard": {
    "ignore": [
      "_*",
      "_**/*",
      "dist/**/*",
      "gulpfile.js"
    ],
    "globals": [
      "chassis",
      "window",
      "EventEmitter",
      "CustomException",
      "CustomEvent",
      "MutationObserver",
      "DOMParser",
      "XMLHttpRequest",
      "FormData",
      "btoa",
      "localStorage",
      "Network",
      "HTMLElement",
      "HTMLInputElement"
    ]
  }
}
